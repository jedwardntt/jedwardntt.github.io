{
	"id": "com.nttdata.ses.cw.gantt",
	"version": "0.0.1",
	"name": "NTT Data Gantt Chart",
	"description": "A custom widget for a gantt chart",
	"newInstancePrefix": "Gantt",
	"icon": "data:image/png;base64,",
	"vendor": "J. Edward Ortiz",
	"eula": "",
	"license": "",
	"webcomponents": [
		{
			"kind": "main",
			"tag": "com-nttdata-ses-cw-gantt",
			"url": "/gantt.js",
			"integrity": "",
			"ignoreIntegrity": true
		}
	],
	"properties": {
		"projectName": {
			"type": "string",
			"description": "Project name",
			"default": "Project name"
		},
		"startDate": {
			"type": "string",
			"description": "Start date",
			"default": "2023-07-022"
		},
		"nttDebug": {
			"type": "boolean",
			"default": true
		},		
		"color": {
			"type": "string",
			"description": "Background color",
			"default": ""
		},
		"opacity": {
			"type": "number",
			"description": "Opacity",
			"default": 1
		},
		"width": {
			"type": "integer",
			"default": 700
		},
		"height": {
			"type": "integer",
			"default": 250
		}		
	},
	"methods": {
		"setProjectName": {
			"description": "Sets the project name.",
			"parameters": [
				{
					"name": "newProjectName",
					"type": "string",
					"description": "The new project name"
				}
			],
			"body": "this.projectName = newProjectName;"
		},
		"getProjectName": {
			"returnType": "string",
			"description": "Returns the project name.",
			"body": "return this.projectName;"
		},
		"setStartDate": {
			"description": "Sets the start date of the project.",
			"parameters": [
				{
					"name": "newStartDate",
					"type": "string",
					"description": "The new start date"
				}
			]
		},
		"getStartDate": {
			"returnType": "string",
			"description": "Returns the project start date.",
			"body": "return this.startDate;"
		},
		"setColor": {
			"description": "Sets the background color.",
			"parameters": [
				{
					"name": "newColor",
					"type": "string",
					"description": "The new background color"
				}
			],
			"body": "this.color = newColor;"
		},
		"getColor": {
			"returnType": "string",
			"description": "Returns the background color.",
			"body": "return this.color;"
		},
		"collapseAllStages": {
			"description": "Collapse all stages.",
			"parameters": [
				{
					"name": "stageId",
					"type": "string",
					"description": "The id if the phase to collapse"
				}
			],
			"body": "this.color = stageId;"
		},
		"expandAllStages": {
			"description": "Expand all stages."
		},
		"collapseStage": {
			"description": "Collapse tasks for a stage.",
			"parameters": [
				{
					"name": "stageId",
					"type": "string",
					"description": "The id if the stage to collapse"
				}
			]
		},
		"expandStage": {
			"description": "Expand tasks for a stage.",
			"parameters": [
				{
					"name": "stageId",
					"type": "string",
					"description": "The id if the phase to expand"
				}
			]
		},
		"hideStage": {
			"description": "Collapse tasks for a stage.",
			"parameters": [
				{
					"name": "stageId",
					"type": "string",
					"description": "The id if the phase to collapse"
				}
			],
			"body": "this.color = stageId;"
		},
		"showStage": {
			"description": "Expand tasks for a stage.",
			"parameters": [
				{
					"name": "stageId",
					"type": "string",
					"description": "The id if the phase to expand"
				}
			],
			"body": "this.color = stageId;"
		},
		"setTaskProperty": {
			"description": "Sets the value for a task property.",
			"parameters": [
				{
					"name": "taskId",
					"type": "string",
					"description": "The id of the task to set the duration"
				},
				{
					"name": "propertyName",
					"type": "string",
					"description": "The name of the property to be updated"
				},
				{
					"name": "newValue",
					"type": "string",
					"description": "The new property value"
				}
			]
		},
		"getTaskProperty": {
			"returnType": "string",
			"description": "Returns the value of the property for the task.",
			"parameters": [
				{
					"name": "taskId",
					"type": "string",
					"description": "The id of the task to set the duration"
				},
				{
					"name": "propertyName",
					"type": "string",
					"description": "The name of the property"
				}
			]
		},
		"getSelectedTaskId": {
			"returnType": "string",
			"description": "Returns the Id of the selected task."
		},
		"setDebugEnabled": {
			"returnType": "boolean",
			"description": "Sets if the debug messages by console log are enabled or disabled.",
			"parameters": [
				{
					"name": "enabled",
					"type": "boolean",
					"description": "True o false"
				}
			]
		},		
		"refresh": {
			"description": "Update the UI to show changes on tasks."
		},
		"reset": {
			"description": "Reset task duration to 0 for all tasks."
		}
	},
	"events": {
		"onClick": {
			"description": "Called when the user clicks the gantt."
		},
		"onDblClick": {
			"description": "Called when the user double clicks the gantt."
		},
		"onContextMenu": {
			"description": "Called when the user right clicks the gantt."
		},
		"onTaskSelected": {
			"description": "Called when the user selects a task."
		}
		 
	}
	
	
}

 
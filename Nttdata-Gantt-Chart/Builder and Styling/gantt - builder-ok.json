{
	"id": "com.nttdata.ses.cw.gantt",
	"version": "0.0.1",
	"name": "Gantt",
	"description": "Gantt chart for estimations",
	"newInstancePrefix": "Gantt",
	"icon": "/icon.jpg",
	"vendor": "J. Edward Ortiz",
	"eula": "",
	"license": "",
	"webcomponents": [
		{
			"kind": "main",
			"tag": "com-nttdata-ses-cw-gantt",
			"url": "/gantt.js",
			"integrity": "",
			"ignoreIntegrity": true
		},
		{
			"kind": "styling",
			"tag": "com-nttdata-ses-cw-gantt-styling",
			"url": "/gantt_styling.js",
			"integrity": "",
			"ignoreIntegrity": true
		},
		{
			"kind": "builder",
			"tag": "com-nttdata-ses-cw-gantt-builder",
			"url": "/gantt_builder.js",
			"integrity": "",
			"ignoreIntegrity": true
		}
	],
	"properties": {
		"color": {
			"type": "string",
			"description": "Background color",
			"default": "red"
		},
		"opacity": {
			"type": "number",
			"description": "Opacity",
			"default": 1
		},
		"width": {
			"type": "integer",
			"default": 700
		},
		"height": {
			"type": "integer",
			"default": 300
		}
	},
	"methods": {
		"setColor": {
			"description": "Sets the background color.",
			"parameters": [
				{
					"name": "newColor",
					"type": "string",
					"description": "The new background color"
				}
			],
			"body": "this.color = newColor;"
		},
		"getColor": {
			"returnType": "string",
			"description": "Returns the background color.",
			"body": "return this.color;"
		}
	},
	"events": {
		"onClick": {
			"description": "Called when the user clicks the gantt."
		}
	}
	
	
}

 